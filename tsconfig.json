{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "jsx": "react",
      "rootDir": "./resources/js",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "paths": {
        "@/*": ["./resources/js/*"]
      },
      "types": ["vite/client"]
    }
}
